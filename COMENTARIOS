El tema de las funciones *_to_string
while ((unsigned int) i < n) { 
			scaux = g_queue_peek_nth (self->scmd, i);
			if (i != 0)
				bcatcstr (result, " |");
			bconcat (result, scommand_to_string(scaux));
			scaux = NULL;
			i++;
}
